mixin traveler-card(props)
  - const { name, flags, tags, rating, image, transport, likes, modifier, isOnline } = props
  li.traveler-card
    article.traveler-card__link
      .traveler-card__image-wrapper
        +picture(image.path, image.image, image.format, image.desktopWidth, image.desktopHeight, image.tabletWidth, image.tabletHeight, image.mobileWidth, image.mobileHeight, image.alt)

      h3.traveler-card__name(class=`traveler-card__name--${isOnline ? 'online' : 'offline'}`)= name

      if likes !== ''
        .traveler-card__likes
          .traveler-card__likes-icon
            +img2("svg", "heart", "svg", 20, 20, "")
          span.traveler-card__likes-count= likes

      p.traveler-card__tags!= tags

      .traveler-card__side-content
        .traveler-card__flags-title
          span хочет&nbsp;посетить:
        .traveler-card__flags-wrapper
          each flag, index in flags
            - const flagName = flag.image.toLowerCase()
            .traveler-card__flag-item(class=`traveler-card__flag-item--${flagName}`)
              .traveler-card__flag
                +picture(flag.path, flag.image, flag.format, 35, 25, 35, 24, 26, 18, flag.alt)
              span.traveler-card__country-name= flag.alt.replace('Флаг ', '').toUpperCase()

      .traveler-card__bottom-content
        .traveler-card__transport-title
          span транспорт:
        ul.traveler-card__transport
          each item in transport
            li.traveler-card__transport-item(class=item.modifier ? `traveler-card__transport-item--${item.modifier}` : '')
              +img2(item.path, item.image, item.format, item.width, item.height, item.alt)
        .traveler-card__level-title
          span левел:
        .traveler-card__rating(class=`traveler-card__rating--${rating}`)
          span.traveler-card__rating-number= rating
          span.traveler-card__rating-text level

      if likes !== ''
        .traveler-card__actions
          button.traveler-card__invite-button(type="button") Позвать!
